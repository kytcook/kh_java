package com.day4;

public class Variable_3 {
	int age = 20;
	void methodA(int age) {
		age = 30;
		System.out.println("methodA : "+age);
	}
	public static void main(String[] args) { //break point
		// 인스턴스화 - 메모리 로딩됨, 상주됨
		// 인스턴스화를 하면 (변수, 메소드) 누릴 수 있다.
		Variable_3 v1 = new Variable_3();
		System.out.println("before : "+v1.age);
		v1.methodA(50); // 메소드를 호출 할땐 ; 붙인다.
		System.out.println("after : "+v1.age);		
	} // end of main - 북마크를 한다.
}
